<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<!DOCTYPE genericSFNTTable [
<!ELEMENT genericSFNTTable (dataLine+)>
<!ELEMENT dataLine EMPTY>
<!ATTLIST genericSFNTTable tag CDATA #REQUIRED
	size CDATA #IMPLIED
>
<!ATTLIST dataLine lineNumber CDATA #REQUIRED
	offset CDATA #REQUIRED
	hex CDATA #REQUIRED
	ascii CDATA #IMPLIED
	unicode CDATA #IMPLIED
>
]>

<!--

	Tracking entries for NarzÄ™dzia Sukcesu the logotype font
	Copyright 2012 Grzegorz Rolek

	Note that the 'trak' table is not currently supported in CoreText.
	For now it is compensated in each of the kerning values.
	In a 1000 UPM environment, the 184 FUnits would be equal to 90.

-->

<genericSFNTTable tag="trak">
	
	<dataLine offset="00000000" hex="00010000" /> <!-- Table Version (1.0) -->
	<dataLine offset="00000004" hex="0000" /> <!-- Table Format -->
	
	<dataLine offset="00000006" hex="000C" /> <!-- Offset to horizontal track data -->
	<dataLine offset="00000008" hex="0000" /> <!-- Offset to vertical track data (zero means none) -->
	<dataLine offset="0000000A" hex="0000" /> <!-- (padding to long word) -->
	
	<dataLine offset="0000000C" hex="0001" /> <!-- Number of tracks -->
	<dataLine offset="0000000E" hex="0002" /> <!-- Number of sizes for each track -->
	<dataLine offset="00000010" hex="0000001C" /> <!-- Offset to the size subtable -->
	
	<dataLine offset="00000014" hex="00000000" /> <!-- Track Number (0.0) -->
	<dataLine offset="00000018" hex="0100" /> <!-- Name table entry for the track (256) -->
	<dataLine offset="0000001A" hex="0024" /> <!-- Offset to tracking values -->
	
	<dataLine offset="0000001C" hex="00090000" /> <!-- Size (9.0 points) -->
	<dataLine offset="00000020" hex="00600000" /> <!-- Size (96.0 points) -->
	
	<dataLine offset="00000024" hex="00B8" /> <!-- Value for track 0 size 1 (9 pt, 184 FUnits) -->
	<dataLine offset="00000026" hex="00B8" /> <!-- Value for track 0 size 2 (96 pt, 184 FUnits) -->
	
</genericSFNTTable>

